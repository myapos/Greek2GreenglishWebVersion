var React = require('react');
var ReactDOM = require('react-dom');

var Hello = React.createClass({
  render: function () {
    return (
      <div>Hello {this.props.name}! Data = {this.props.data}</div>
    )
  }
});

ReactDOM.render(<Hello name="Myron" data={"sdcsdcsd"}/>, document.getElementById('app'));
